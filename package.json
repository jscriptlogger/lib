{
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.x",
    "@types/chai": "4.x",
    "@types/node": "18.x",
    "@types/sinon": "10.x",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "chai": "4.x",
    "eslint": "8.x",
    "nyc": "15.x",
    "prettier": "2.x",
    "sarg": "5.x",
    "sinon": "15.x",
    "ts-node": "10.x",
    "typescript": "5.x"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "jsbuffer": "0.x",
    "long": "5.x"
  },
  "scripts": {
    "test": "TS_NODE_PROJECT=test/tsconfig.json npx sarg --bail --require ts-node/register \"test/**/*.ts\"",
    "test:coverage": "npx nyc --exclude-node-modules yarn test",
    "schema": "yarn run schema:build && yarn run schema:lint",
    "schema:build": "npx jsbuffer schema -o schema.out --extends tsconfig.base.json --indentation-size 2",
    "schema:lint": "npx prettier --write schema.out",
    "prepublishOnly": "npx tsc -b node browser src --force"
  },
  "files": [
    "src/**/*.{js,map,d.ts}",
    "schema.out/**/*.{js,map,d.ts}",
    "node/**/*.{js,map,d.ts}"
  ],
  "version": "0.0.9",
  "license": "MIT",
  "name": "js-log-time"
}
